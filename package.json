  },
  "devDependencies": {
    "@angular/cli": "^1.4.9",
    "@ionic/app-scripts": "3.0.1",
    "codelyzer": "^3.2.2",
    "husky": "^0.14.3",
    "leasot": "^4.10.2",
    "lint-staged": "^4.3.0",
    "ngrx-store-freeze": "^0.2.0",
    "prettier": "^1.7.4",
    "tslint": "^5.8.0",
    "tslint-ionic-rules": "0.0.11",
    "typescript": "2.3.4"
  },
  "config": {
    "ionic_webpack": "./webpack.config.js"
  }
}
